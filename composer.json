{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gordonbanderson/flickr-1.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/gordonbanderson/flickr-cli.git"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "^4",
        "silverstripe/googlesitemaps": "^2.0",
        "silverstripe/blog": "^3.0",
        "silverstripe/widgets": "^2.0",
        "tractorcow/silverstripe-opengraph": "^4.0",
        "alek13/slack": "2.x-dev",
        "colymba/gridfield-bulk-editing-tools": "3.0.0-beta4",
        "jonom/focuspoint": "^3.0",
        "benmanu/silverstripe-simple-styleguide": "^2.0",
        "weboftalent/twitter-tools": "^2",
        "weboftalent/silverstripe-shortcode-gist": "dev-master",
        "weboftalent/gridrows": "^2.0",
        "suilven/homepage": "^1",
        "suilven/homepage-ss4-bootstrap-theme": "dev-flattern",
        "suilven/blog-ss4-bootstrap-theme": "dev-flattern",
        "suilven/default-ss4-bootstrap-theme": "dev-flattern",
        "ezyang/htmlpurifier": "^4.10",
        "silverstripe/comments": "^3.1",
        "suilven/silverstripe-notifier": "^1.0",
        "suilven/comments-chat-notifications": "^1.0",
        "symbiote/silverstripe-queuedjobs": "^4.1",
        "silverstripers/markdown": "^2",
        "axllent/simplehtmldom": "^1.5",
        "silverstripe/auditor": "dev-master",
        "suilven/audit-chat-notifications": "dev-master",
        "foolz/sphinxql-query-builder": "^1.2",
        "suilven/comments-ss4-bootstrap-theme": "dev-flattern",
        "ripaclub/sphinxsearch": "^0.8.1",
        "silverstripe/elemental-blocks": "1.0.x-dev",
        "wampiedriessen/phpflickr": "dev-master",
        "guzzle/http": "^3.9",
        "php-vcr/php-vcr": "^1.4",
        "simplepie/simplepie": "^1.5",
        "suilven/blog-extras": "^1.0",
        "dynamic/silverstripe-elemental-gallery": "^1.0",
        "suilven/utils-theme": "^0.0.5",
        "bramdeleeuw/cookieconsent": "^2.0",
        "suilven/template-env-checker": "^1.0@dev",
        "ryanpotter/silverstripe-cms-theme": "^3.3",
        "weboftalent/cachekeyhelper": "^2.0",
        "weboftalent/links": "^6.0",
        "rezzza/flickr": "dev-ss4",
        "thefox/flickr-cli": "dev-ss4",
        "smindel/silverstripe-gis": "dev-master",
        "silverstripe/postgresql": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.2",
        "suilven/silverstripe-configurator": "dev-master",
        "dnadesign/silverstripe-populate": "dev-master",
        "phpstan/phpstan": "^0.9.2",
        "lekoala/silverstripe-debugbar": "^2.0"
    },
    "extra": {
        "branch-alias": {
            "4.x-dev": "4.2.x-dev",
            "dev-master": "5.x-dev"
        },
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php",
            "mysite/.htaccess",
            "mysite/_config.php",
            "mysite/_config/mysite.yml",
            "mysite/code/Page.php",
            "mysite/code/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
